<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cn.itsite.aguider.demo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".demo.NextActivity" />
        <activity android:name=".demo.ActivityActivity" />
        <activity android:name=".demo.FragmentActivity" />
        <activity android:name=".demo.TogetherActivity" />
        <activity android:name=".demo.ListActivity" />
        <activity android:name=".demo.AnywhereActivity"/>
    </application>

</manifest>